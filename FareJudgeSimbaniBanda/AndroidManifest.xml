            android:name="com.example.FareJudgeApp.FareJudgeWelcomePage"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
         <activity android:name="com.example.FareJudgeApp.MainActivity"></activity>
         <activity android:name="com.example.FareJudgeApp.Listdetails"></activity>
        <activity android:name="com.example.FareJudgeApp.Search"></activity>
         <activity android:name="com.example.FareJudgeApp.Review"></activity>
        <activity android:name="com.example.FareJudgeApp.ReviewList"></activity>
        
        
        <!-- Single List Item Activity -->
        <activity
            android:label="Contact"
            android:name="com.example.farejudgeshelakatongo.jsonparsing.SingleContactActivity" >
        </activity>
        
        
    </application>

</manifest>
